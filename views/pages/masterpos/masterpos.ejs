<!-- Main Content Area -->
<div class="page-holder">

  <!-- ENTRIES CALLER -->
  <div class="col-12 ml-auto mt-3 d-flex justify-content-end button-holder">
    <!-- SHOW ENTRIES -->
    <button class="btn btn-dark mr-1" id="showButton2">Entries</button>
    <!-- HIDE ENTRIES -->
    <button class="btn btn-danger" id="hideButton2">Close</button>
  </div>

  <!-- ENTRIES -->
  <div class="mt-3" id="targetDiv2" style="display: none;">
    <!-- PAGE TITLE -->
    <div class="col-sm-12 top-title-bar">
      <label id="tittle-bar">POS Entries</label>
    </div>

    <!-- ENTRIES CONTAINER -->
    <div class="col-md-12 entry-container">
      <div>
        <!-- <form class="needs-validation combined-form" novalidate> -->
        <div class="row">
          <div class="col-md-4 mb-3">
            <label for="name">POS Name</label>
            <input id="name" name="name" type="text" class="form-control" placeholder="POS Name"
              required />
          </div>

          <div class="col-md-4 mb-3">
            <label for="serial">Full Name</label>
            <input id="serial" name="serial" type="text" class="form-control" placeholder="Serial"
              required />
          </div>

          <div class="col-md-4 mb-3">
            <label for="min">User Name</label>
            <input id="min" name="min" type="text" class="form-control" placeholder="MIN" required />
          </div>
        </div>

        <div class="row justify-content-center">
          <div class="col-md-4 mb-3">
            <label for="ptu">PTU</label>
            <input id="ptu" name="ptu" type="text" class="form-control" placeholder="PTU" required />
          </div>
        </div>

        <div class="row justify-content-end mr-4">
          <button id="addBtn" class="btn btn-success" type="submit">+ Add</button>
        </div>
        <!-- </form> -->
      </div>
    </div>
    <!-- CONTAINER END -->
  </div>
  <!-- END ENTRIES -->

  <!-- TABLE TITLE -->
  <div class="col-sm-12 mid-title-bar">
    <label id="tittle-bar">POS Table</label>
  </div>

  <!-- TABLE -->
  <div class="table-responsive scrollable-table-container-lg">
    <table id="data-table" class="table custom-mobile-table">
      <thead class="table-header">
        <th class="no-wrap">ID</th>
        <th class="no-wrap">POS Name</th>
        <th class="no-wrap">Serial</th>
        <th class="no-wrap">MIN</th>
        <th class="no-wrap">PTU</th>
        <th class="no-wrap">Status</th>
        <th class="no-wrap">Actions</th>
      </thead>
      <tbody></tbody>
    </table>
  </div>


</div>